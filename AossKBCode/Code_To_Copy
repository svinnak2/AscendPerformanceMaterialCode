import boto3

# Initialize S3 client
s3 = boto3.client('s3')

# List all objects with prefix
bucket_name = "ABC"
prefix = "PD/LKM/"
response = s3.list_objects_v2(Bucket=bucket_name, Prefix=prefix)

# Filter and print only JSON files
json_files = [f"s3://{bucket_name}/{item['Key']}" for item in response.get('Contents', []) if item['Key'].endswith('.json')]
for json_file in json_files:
    print(json_file)
