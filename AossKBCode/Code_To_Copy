import re

# List of input strings
strings = [
    "RFPPDF?rpt=FF101&id=102&dt=2024",
    "RFPPDF?rpt=YP102&id=102&dt=2023",
    "RFPPDF?rpt=ZK61&id=102&dt=2025"
]

# Function to convert the string
def convert_string(input_string):
    # Use regex to extract the desired parts
    match = re.search(r"rpt=([^&]*)&id=([^&]*)&dt=([^&]*)", input_string)
    if match:
        # Combine the extracted parts with underscores
        return f"{match.group(1)}_{match.group(2)}_{match.group(3)}"
    else:
        return None  # Return None if no match is found

# Apply the function to each string
converted_strings = [convert_string(s) for s in strings]

# Print the results
print(converted_strings)
